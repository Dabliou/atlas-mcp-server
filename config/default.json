{
  "env": "development",
  "storage": {
    "name": "atlas-tasks",
    "connection": {
      "maxRetries": 3,
      "retryDelay": 1000,
      "busyTimeout": 5000
    },
    "performance": {
      "checkpointInterval": 30000,
      "cacheSize": 2000,
      "mmapSize": 33554432,
      "pageSize": 4096,
      "maxMemory": 134217728,
      "sharedMemory": false
    },
    "journalMode": "wal",
    "synchronous": "normal",
    "tempStore": "memory"
  },
  "logging": {
    "console": true,
    "file": true,
    "level": "debug",
    "maxFiles": 10,
    "maxSize": 10485760
  }
}
